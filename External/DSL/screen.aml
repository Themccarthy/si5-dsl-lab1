app RedButton initial state off {
	bricks
		Actuator red_led : 12
        Actuator red_led2 : 11
		Sensor button : 9
        Screen screen : 1 
	
	states
		off {
			red_led <= LOW
			red_led2 <= LOW
            screen <= eteint 
			button is HIGH => on 
		}
		on {
			red_led <= HIGH
			red_led2 <= HIGH
			screen <= allume
			button is HIGH => off
		}
}

