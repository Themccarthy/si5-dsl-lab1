app RedButton initial state off {
	bus
		Bus 1 {1,2,3,5}
        Bus 1 {1,2,3,5,12}

	bricks
		Actuator red_led 
        Actuator red_led2 
		Sensor button 
	
	states
		off {
			red_led <= LOW
            red_led2 <= LOW
			button is HIGH => on 
		}
		on {
            red_led2 <= HIGH
			red_led <= HIGH
			button is LOW => off
		}
}

