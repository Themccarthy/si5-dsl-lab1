app RedButton initial state off {
	pins
		Pin digital_1 : 26 : DIGITAL_OUTPUT
		Pin digital_2 : 26 : DIGITAL_OUTPUT
		Pin digital_2 : 22 : DIGITAL_INPUT
		Pin analog_1 : 3 : ANALOG_INPUT
		Pin analog_2 : 4 : ANALOG_OUTPUT

	bus
		Bus 1 {21,22,23,25,26,27}
		Bus 1 {1,2,3,5,6,7,8}

	bricks
		Actuator red_led : 26
        Actuator red_led2 
		Sensor button 
	
	states
		off {
			red_led <= LOW
            red_led2 <= LOW
			button is HIGH => on 
		}
		on {
            red_led2 <= HIGH
			red_led <= HIGH
			button is LOW => off
		}
}

